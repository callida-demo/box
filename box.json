{
 "id": "goverp.sample.box",
 "version": "1.0.0",
 "name": "Box",
 "description": "A coloured box",
 "newInstancePrefix": "Box",
 "icon": "https://callida-demo.github.io/box/Excel-icon.png",  
 "vendor": "GOVERP",
 "eula": "",
 "license": "",
 "webcomponents": [
 {
 "kind": "main",
 "tag": "goverp-sample-box",
 "url": "https://callida-demo.github.io/box/box.js",
 "integrity": "",
 "ignoreIntegrity": true
 },
 {
 "kind": "styling",
 "tag": "goverp-sample-box-styling",
 "url": "https://callida-demo.github.io/box/box_styling.js",
 "integrity": "",
 "ignoreIntegrity": true
 },
 {
 "kind": "builder",
 "tag": "goverp-sap-sample-box-builder",
 "url": "https://callida-demo.github.io/box/box_builder.js",
 "integrity": "",
 "ignoreIntegrity": true
 }
 ],
 "properties": {
 "color": {
 "type": "string",
 "description": "Background color",
 "default": "red"
 },
 "opacity": {
 "type": "number",
 "description": "Opacity",
 "default": 1
 },
 "width": {
 "type": "integer",
 "default": 100
 },
 "height": {
 "type": "integer",
 "default": 100
 },
  "fileData": {
   "type" = "string[]"
  }
 },
 "methods": {
 "setColor": {
 "description": "Sets the background color.",
 "parameters": [
 {
 "name": "newColor",
 "type": "string",
 "description": "The new background color"
 }
 ],
 "body": "this.color = newColor;"
 },
 "getColor": {
 "returnType": "string",
 "description": "Returns the background color.",
 "body": "return this.color;"
 }
 },
 "getFileData": {
 "returnType": "string[]",
 "description": "Returns the file data.",
 }
 },

 "events": {
 "onClick": {
 "description": "Called when the user clicks the Colored Box."
 }
 }
}
